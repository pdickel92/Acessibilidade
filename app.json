{
  "expo": {
    "name": "Acessibilidade",
    "slug": "Acessibilidade",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cegoicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/inicio.gif",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Permita o acesso à câmera.",
        "NSPhotoLibraryUsageDescription": "Permissão.",
        "NSPhotoLibraryAddUsageDescription": "Permissão"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_MEDIA_LOCATION"
      ],
      "package": "com.petersondickel.Acessibilidade"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3486d171-5b40-4634-98b5-f89402eae846"
      }
    }
  }
}
